            />
        <activity
            android:name=".setting.AboutActivity"
            android:exported="false"
            />

        <activity
            android:name=".jetpack.navigation.NavSettingActivity"
            android:exported="false"
            />

        <activity
            android:name=".demo.cheese.CheeseActivity"
            android:exported="false"
            />
        <activity
            android:name=".demo.cheese.CheeseDetailActivity"
