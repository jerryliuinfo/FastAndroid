<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <LinearLayout
        android:id="@+id/article_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
       >

        <include layout="@layout/custom_bar"

            />

        <Button
            android:textColor="@color/white"
            android:layout_marginTop="30dp"
            android:text="调用Js，无参数方法"
            android:textSize="14sp"
            android:id="@+id/callJsNoParamsButton"
            android:layout_marginLeft="20dp"
            android:textAllCaps="false"
            android:layout_marginRight="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:text="调用Js，一个参数方法"
            android:textAllCaps="false"
            android:textSize="14sp"
            android:id="@+id/callJsOneParamsButton"
            android:textColor="@color/white"
            android:layout_marginRight="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:text="调用Js，多个参数方法"
            android:textSize="14sp"
            android:id="@+id/callJsMoreParamsButton"
            android:textColor="@color/white"
            android:layout_marginTop="10dp"
            android:textAllCaps="false"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Button
            android:text="Android-Js-Android"
            android:textSize="14sp"
            android:id="@+id/jsJavaCommunicationButton"
            android:textColor="@color/white"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_width="match_parent"
            android:textAllCaps="false"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp" />

    </LinearLayout>


</layout>
