<manifest package="com.apache.fastandroid.topic"
          xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
>

    <uses-sdk tools:overrideLibrary="com.android.support:multidex:1.0.2"/>



    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat"
        >


        <activity android:name=".TopicHomeActivity"/>


        <provider
            android:name=".TopicProvider"
            android:authorities="PROVIDER_TOPIC"/>

    </application>

</manifest>
